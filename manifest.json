{
	"manifest_version": 2,

	"name": "YouTube Thumbnail Extractor",
	"description": "This extension allows for easy extraction of YouTube thumbnails.",
	"version": "1.0",

	"permissions": [
		"tabs",
		"http://img.youtube.com/",
		"https://www.youtube.com/"
	],
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": ["background.js"],
		"persistent": true
	}
}
