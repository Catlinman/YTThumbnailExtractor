{
	"manifest_version": 2,
	"name": "YouTube Thumbnail Extractor",
	"description": "This extension allows for easy extraction of YouTube thumbnails.",
	"version": "1.0",
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"browser_action": {
		"default_icon": "icon128.png",
		"default_popup": "popup.html"
	},
	"author": "Catlinman",
	"homepage_url": "http://catlinman.com",
	"permissions": [
		"tabs",
		"http://img.youtube.com/",
		"https://www.youtube.com/"
	],
	"background": {
		"scripts": ["background.js"],
		"persistent": true
	}
}
